					pieHole: 0.4,
          pieSliceText: 'none',
          pieStartAngle: 135,
          tooltip: { trigger: 'none' },
          slices: {
            0: { color: '#66ff33' },
            1: { color: '#ffff66' },
						2: { color: '#ff3300' }
          }
        };

