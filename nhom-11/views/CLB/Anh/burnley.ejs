      function drawChart() {
        var data = google.visualization.arrayToDataTable([
          ['Trận đấu', 'Percentage'],
          ['Thắng', 75],
          ['Hòa', 25],
					['Thua', 25]
        ]);

        var options = {
					pieHole: 0.4,
          pieSliceText: 'none',
          pieStartAngle: 135,
          tooltip: { trigger: 'none' },
          slices: {
            0: { color: '#66ff33' },
            1: { color: '#ffff66' },
						2: { color: '#ff3300' }
          }
        };

        var chart = new google.visualization.PieChart(document.getElementById('sannha'));
        chart.draw(data, options);
      }
    </script>
